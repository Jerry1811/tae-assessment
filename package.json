  },
  "devDependencies": {
    "cypress": "^10.9.0",
    "cypress-multi-reporters": "^1.6.1",
    "eslint-plugin-cypress": "^2.12.1",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "npm-run-all": "^4.1.5"
  }
}
